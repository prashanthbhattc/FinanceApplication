<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/css/dashboard.css">

    <style>
/* Center the loader */

        </style>
</head>
<body>
    <nav class="navbar navbar-dark  fixed-top bg-info flex-md-nowrap p-0 shadow">
        <a class="navbar-brand col-sm-3 col-md-2 mr-0" href="#">Finance</a>
        <ul class="navbar-nav px-3">
        </ul>
    </nav>

    <div class="container-fluid">
        <partial name="_navigation" />
        <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">

            @RenderBody()
        </main>

    </div>




    <partial name="_ModalSkeliton" />

    <partial name="_Loading" />


    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>


    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/feather-icons/4.9.0/feather.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.3/Chart.min.js"></script>
    <script>
        (function () {
            'use strict'

            feather.replace()
        })();
    </script>
    <script src="~/js/Campaign/Campaign.js"></script>

    <script src="~/lib/microsoft/signalr/dist/browser/signalr.js" defer></script>
    <script src="~/js/Common/Constants.js"></script>
    <script src="~/js/Notification/EnquiryNotification.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
